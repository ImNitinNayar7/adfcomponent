<?xml version = '1.0' encoding = 'windows-1252'?>
<facelet-taglib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0" xmlns="http://java.sun.com/xml/ns/javaee">
    <namespace>http://www.redheap.com/rh</namespace>
    <tag>
        <tag-name>multiSelect</tag-name>
        <component>
            <component-type>com.redheap.rh.componenttype.MultiSelect</component-type>
            <renderer-type>com.redheap.rh.rendertype.MultiSelect</renderer-type>
            <handler-class>com.redheap.rh.faces.component.RedheapComponentHandler</handler-class>
        </component>
        <!-- need to list all attributes here for editor (and code completion) in JDeveloper -->
        <attribute>
            <name>styleClass</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>id</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>value</name>
            <type>java.util.List</type>
        </attribute>
        <attribute>
            <name>itemSelectListener</name>
            <type>javax.el.MethodExpression</type>
            <!-- method signature for JDeveloper design time support -->
            <method-signature>void myMethod(com.redheap.rh.faces.event.ItemSelectEvent)</method-signature>
        </attribute>
    </tag>
</facelet-taglib>
